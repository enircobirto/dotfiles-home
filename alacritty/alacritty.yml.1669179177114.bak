window:
  opacity: 0.8
  padding:
    x: 15
    'y': 15
  dimensions:
    columns: 120
    lines: 40
  decorations: full
scrolling:
  history: 100000
font:
  normal:
    family: Iosevka Curly
    style: Regular
  bold:
    family: Jetbrains Mono
    style: Bold
  italic:
    family: Iosevka
    style: Italic
  bold_italic:
    family: iosevka
    style: Bold Italic
  size: 8.5
selection:
  save_to_clipboard: true
shell:
  program: /usr/bin/fish
colors:
  name: Isotope (dark)
  author: Chris Kempson
  primary:
    background: '0x1f1f1f'
    foreground: '0xc0b18b'
  cursor:
    text: '#000000'
    cursor: '#d0d0d0'
  normal:
    black: '0x4a3637'
    red: '0xd17b49'
    green: '0x7b8748'
    yellow: '0xaf865a'
    blue: '0x535c5c'
    magenta: '0x775759'
    cyan: '0x6d715e'
    white: '0xc0b18b'
  bright:
    black: '0x4a3637'
    red: '0xd17b49'
    green: '0x7b8748'
    yellow: '0xaf865a'
    blue: '0x535c5c'
    magenta: '0x775759'
    cyan: '0x6d715e'
    white: '0xc0b18b'
theme: Isotope.dark
